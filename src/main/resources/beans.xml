<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   <bean id="doc" class="com.spring_doctor.Doctor">
   <property name="name" value="Sachin"/>
   <property name="degree" value="MBBS"/>
   <property name="p1"> <ref bean="pp"/> </property>
   </bean>
   
   <bean id="pp" class="com.spring_doctor.Patient">
   <property name="patientName" value="Sham"/>
   <property name="desease" value="Covid19"/>
    </bean>
   
   
   
   
   </beans>
